<!-- 		WHERE FAQ_TITLE = '테스트8' AND DBMS_LOB.COMPARE(FAQ_CONTENT, #{ faqContent } ) = 0 AND FAQ_STATUS = 'Y' -->
<!-- 	</select> -->
	
	<select id="getFaqList" resultType="Faq">
		SELECT * FROM FAQ_TBL WHERE FAQ_STATUS = 'Y'
	</select>
</mapper>